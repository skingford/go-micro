{
  "printWidth": 100,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "none",
  "arrowParens": "avoid",
  "overrides": [
    {
      "files": ["*.wxss", "*.acss"],
      "options": {
        "parser": "css"
      }
    },
    {
      "files": ["*.wxml", "*.axml"],
      "options": {
        "parser": "html"
      }
    }
  ]
}
